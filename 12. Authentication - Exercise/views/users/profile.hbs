<h1>Profile</h1>
<section>
    <h2>Username: {{../currentUser.username}}</h2>
</section>
<section>
    <h2>Rent history</h2>
    <table>
        <thead>
            <tr>
                <td>Car</td>
                <td>Back when you want</td>
            </tr>
        </thead>
        <tbody>
            {{#each rentings}}
                <tr>
                    <td>{{car.make}} {{car.model}}</td>
                    <td>Not Implemented</td>
                </tr>
            {{else}}
                <tr>
                    <td colspan="2">
                         <i>No rent history. 
                             <a href="/cars/all">Rent now</a>
                         </i>    
                    </td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</section>